<div class="container">
  <%= simple_form_for(@place) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.input :description %>
    <%= f.input :price_per_day %>
    <%= f.input :capacity %>
    <%= f.submit %>
  <% end %>
</div>